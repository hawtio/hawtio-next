  "devDependencies": {
    "eslint-config-react-app": "^7.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "replace": "^1.2.2",
    "ts-jest": "^29.0.5",
    "tsup": "^6.7.0"
  },
  "peerDependencies": {
    "@patternfly/react-core": "^4.276.8",
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
